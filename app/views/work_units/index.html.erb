<p id="notice"><%= notice %></p>

<h1>Work Units</h1>

<table>
  <thead>
    <tr>
      <th>Nama</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @work_units.each do |work_unit| %>
      <tr>
        <td><%= work_unit.nama %></td>
        <td><%= link_to 'Show', work_unit %></td>
        <td><%= link_to 'Edit', edit_work_unit_path(work_unit) %></td>
        <td><%= link_to 'Destroy', work_unit, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Work Unit', new_work_unit_path %>
